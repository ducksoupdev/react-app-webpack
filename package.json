    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
